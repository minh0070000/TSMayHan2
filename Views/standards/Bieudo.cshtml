@{
}
@model List<string>

<form asp-controller="standards" asp-action="ShowData" method="post">
    <select name="selectedProperty">
        @foreach (var property in Model)
        {
            <option value="@property">@property</option>
        }
    </select>
    <input type="submit" value="Show Data" />
</form>
